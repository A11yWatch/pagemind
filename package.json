{
  "name": "pagemind",
  "version": "0.0.1",
  "description": "Get metrics on your web page with inclusion",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "lint": "eslint .",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/server.js"
  },
  "engines": {
    "node": "v14.x"
  },
  "dependencies": {
    "@a11ywatch/website-source-builder": "^0.0.11",
    "body-parser": "^1.19.0",
    "cors": "2.8.0",
    "date-fns": "2.27.0",
    "dotenv": "8.2.0",
    "express": "4.17.2",
    "generic-pool": "3.8.2",
    "get-page-speed": "^1.0.1",
    "lighthouse": "^9.4.0",
    "node-fetch": "2.6.0",
    "pa11y": "6.1.1",
    "prettier": "^2.5.1",
    "puppeteer": "^13.4.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.10.2",
    "@types/pa11y": "^5.3.0",
    "babel-jest": "^26.6.3",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.4.1",
    "jest": "^26.4.2",
    "jest-config": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "3.8.3",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "verbose": true,
    "preset": "ts-jest",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ]
}
