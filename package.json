{
  "name": "pagemind",
  "version": "0.0.2",
  "description": "Get metrics on your web page with inclusion",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "lint": "eslint .",
    "build": "tsc && tsc-alias && cp -R src/proto/*.proto dist/proto/",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@a11ywatch/website-source-builder": "^0.0.21",
    "@grpc/grpc-js": "^1.6.2",
    "@grpc/proto-loader": "^0.6.9",
    "get-page-speed": "^1.0.1",
    "lighthouse": "9.5.0",
    "litepa11y": "0.4.0",
    "puppeteer": "^13.7.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.117",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.10.2",
    "@types/pa11y": "^5.3.0",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.4.1",
    "jest": "^25.5.1",
    "jest-config": "^26.6.3",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.6.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ]
}
